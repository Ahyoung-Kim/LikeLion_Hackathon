act';\nimport { \n  LoginDiv, LoginBtn, LoginBtnDiv,\n  RegisterText, RegisterUl, RegisterLi,\n  RegisterInput, RegisterInputText,\n  RegisterBtn\n} from '../../styledComponents';\nimport axios from 'axios'\nimport { APIURL } from '../../config/key';\n\nconst LoginInput = () => {\n  const [input, setInput] = useState({\n    email: '',\n    pwd: ''\n  })\n  const { email, pwd } = input;\n  const emailRef = useRef();\n  const pwdRef = useRef();\n\n  useEffect(() => {\n    emailRef.current.focus();\n  }, []);\n\n  const onChange = (e) => {\n    const { name, value } = e.target;\n\n    setInput({\n      ...input,\n      [name]: value\n    })\n  }\n  const onKeyUp = (e) => {\n    const { name } = e.target;\n\n    if(e.key === 'Enter'){\n      if(name === 'email'){\n        pwdRef.current.focus();\n      } else {\n        onLogin();\n      }\n    }\n  }\n\n  const sendRequest = async() => {\n    const res = await axios.post(`${APIURL}/account/login/`, {\n      email: email,\n      password: pwd\n    })\n\n    console.log(res);\n    if(res.data.status_code == 200){\n      console.log('login success')\n    } else {\n      console.log('login fail')\n    }\n  }\n\n  const onLogin = () => {\n    if(!email || !pwd){\n      alert('모든 정보를 입력해주세요!');\n    } else {\n      sendRequest();\n    }\n  }\n\n  return (\n    <>\n      <LoginDiv>\n\n        <RegisterText>\n          당신의 스토리를 알려주세요!\n        </RegisterText>\n\n        <RegisterUl>\n          <RegisterLi>\n            <RegisterInputText>이메일</RegisterInputText>\n            <RegisterInput \n              type=\"text\"\n              name=\"email\"\n              value={email}\n              ref={emailRef}\n              onChange={onChange}\n              onKeyUp={onKeyUp}\n            />\n          </RegisterLi>\n          <RegisterLi>\n            <RegisterInputText>비밀번호</RegisterInputText>\n            <RegisterInput \n              type=\"password\"\n              name=\"pwd\"\n              value={pwd}\n              ref={pwdRef}\n              onChange={onChange}\n              onKeyUp={onKeyUp}\n            />\n          </RegisterLi>\n        </RegisterUl>\n\n        <RegisterBtn onClick={onLogin}>\n          로그인\n        </RegisterBtn>\n\n        <LoginBtnDiv>\n          <LoginBtn>아이디 찾기</LoginBtn>\n          <LoginBtn>비밀번호 찾기</LoginBtn>\n        </LoginBtnDiv>\n\n      </LoginDiv>\n    </>\n  );\n};\n\nexport default React.memo(LoginInput);","/Users/changho/Desktop/LikeLion_Hackathon/client/devs/src/components/profile/ProfileImage.jsx",[],[],"/Users/changho/Desktop/LikeLion_Hackathon/client/devs/src/components/profile/ProfileFollow.jsx",[],[],"/Users/changho/Desktop/LikeLion_Hackathon/client/devs/src/components/profile/ProfileText.jsx",[],[],"/Users/changho/Desktop/LikeLion_Hackathon/client/devs/src/components/profile/DescHead.jsx",[],[],"/Users/changho/Desktop/LikeLion_Hackathon/client/devs/src/components/profile/ProfileTag.jsx",[],[],"/Users/changho/Desktop/LikeLion_Hackathon/client/devs/src/config/key.js",[],[],{"ruleId":"194","severity":1,"message":"195","line":1,"column":17,"nodeType":"196","messageId":"197","endLine":1,"endColumn":26},{"ruleId":"194","severity":1,"message":"198","line":1,"column":28,"nodeType":"196","messageId":"197","endLine":1,"endColumn":36},{"ruleId":"194","severity":1,"message":"199","line":3,"column":48,"nodeType":"196","messageId":"197","endLine":3,"endColumn":59},{"ruleId":"194","severity":1,"message":"200","line":5,"column":28,"nodeType":"196","messageId":"197","endLine":5,"endColumn":35},{"ruleId":"194","severity":1,"message":"201","line":5,"column":37,"nodeType":"196","messageId":"197","endLine":5,"endColumn":43},{"ruleId":"194","severity":1,"message":"202","line":5,"column":45,"nodeType":"196","messageId":"197","endLine":5,"endColumn":51},{"ruleId":"203","severity":1,"message":"204","line":68,"column":9,"nodeType":"205","endLine":71,"endColumn":31},{"ruleId":"194","severity":1,"message":"206","line":3,"column":15,"nodeType":"196","messageId":"1