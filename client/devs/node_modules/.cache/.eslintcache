scDiv>\n                <CareerText>성과</CareerText>\n                <CareerDesc>\n                  콘텐츠 공유 기능, 직무 맞춤 콘텐츠 추천 기능 개발 → 구독 전환율 개선에 기여\n                </CareerDesc>\n              </CareerDescDiv>\n              <CareerDescDiv>\n                <CareerText>사용 기술</CareerText>\n                <CareerDesc>Next.js, React-query</CareerDesc>\n              </CareerDescDiv>\n            </CareerDiv>\n          </DescContentsDiv>\n        </DescDiv>\n\n      </ProfileDiv>\n    </>\n  );\n};\n\nexport default ProfilePage;","/Users/changho/Desktop/LikeLion_Hackathon/client/devs/src/components/profile/profile3/ProfilePage3.jsx",["170","171","172","173","174"],[],"/Users/changho/Desktop/LikeLion_Hackathon/client/devs/src/styledComponents.js",[],[],"/Users/changho/Desktop/LikeLion_Hackathon/client/devs/src/components/landing/LandingTextComp.jsx",[],[],"/Users/changho/Desktop/LikeLion_Hackathon/client/devs/src/components/landing/LandingInfo.jsx",[],[],"/Users/changho/Desktop/LikeLion_Hackathon/client/devs/src/components/landing/Register.jsx",["175","176","177","178"],[],"import axios from 'axios';\nimport React, { useState, useEffect, useRef } from 'react';\nimport { useNavigate } from 'react-router-dom';\nimport { APIURL } from '../../config/key';\nimport { \n  RegisterDiv, RegisterText, RegisterUl,\n  RegisterLi, RegisterInputText, RegisterInput,\n  RegisterBtn, AlreadyRegister, WrongInputText\n} from '../../styledComponents';\n\nconst RegTextComp = React.memo(() => (\n  <RegisterText>\n    지금 바로 가입하세요!\n  </RegisterText>\n))\n\nconst Register = () => {\n  // 이메일 유효성 검사\n  const [isRightEmail, setIsRightEmail] = useState(true);\n  // 비밀번호 8자 미만일 시 true -> 비밀번호 재입력칸 비활성화\n  const [isPwdRight, setIsPwdRight] = useState(false);\n  // 비밀번호 재입력 일치하지 않았을 때\n  const [isPwdDiff, setIsPwdDiff] = useState(false);\n\n  const [input, setInput] = useState({\n    uname: '',\n    email: '',\n    nickname: '',\n    pwd: '',\n    pwd2: ''\n  })\n  const { uname, email, nickname, pwd, pwd2 } = input;\n  const unameRef = useRef();\n  const emailRef = useRef();\n  const nickRef = useRef();\n  const pwdRef = useRef();\n  const pwd2Ref = useRef();\n\n  // 이메일 유효성 검사\n  useEffect(() => {\n    if(!email){\n      setIsRightEmail(true)\n      return\n    }\n\n    var regExp = /^[0-9a-zA-Z]([-_\\.]?[0-9a-zA-Z])*@[0-9a-zA-Z]([-_\\.]?[0-9a-zA-Z])*\\.[a-zA-Z]{2,3}$/i\n    // 형식에 맞는 경우 true 리턴\n    //console.log('이메일 유효성 검사 :: ', regExp.test(email))\n\n    if(regExp.test(email)){\n      setIsRightEmail(true)\n    } else {\n      setIsRightEmail(false)\n    }\n\n  }, [email])\n\n  // 비밀번호 유효성 검사\n  useEffect(() => {\n    if(!pwd){\n      setIsPwdRight(true);\n      return;\n    }\n\n    var regExp = /^(?=.*\\d)(?=.*[a-zA-Z])[0-9a-zA-Z]{8,16}$/\n    // 형식에 맞는 경우 true 리턴\n    //console.log('비밀번호 유효성 검사 :: ', regExp.test(pwd))\n\n    if(regExp.test(pwd)){\n      setIsPwdRight(true);\n    } else {\n      setIsPwdRight(false);\n    }\n\n  }, [pwd])\n\n  // 비밀번호 재입력 유효성 검사\n  useEffect(() => {\n    if(pwd2 === ''){\n      setIsPwdDiff(false)\n      return\n    }\n\n    if(pwd2 !== pwd){\n      setIsPwdDiff(true)\n    } else {\n      setIsPwdDiff(false)\n    }\n  }, [pwd2])\n\n  const onChange = (e) => {\n    const { name, value } = e.target;\n\n    setInput({\n      ...input,\n      [name]: value\n    })\n  }\n\n  const onKeyUp = (e) => {\n    const { name } = e.target;\n\n    if(e.key === 'Enter'){\n      if(name === \"uname\"){\n        emailRef.current.focus();\n      } else if(name === 'email'){\n        nickRef.current.focus();\n      } else if(name === 'nickname'){\n        pwdRef.current.focus();\n      } else if(name === 'pwd'){\n        pwd2Ref.current.focus();\n      } else if(name === 'pwd2'){\n        onRegister();\n      }\n    }\n  }\n\n  const sendRequest = async() => {\n    const res = await axios.post(`${APIURL}/account/signup/`, {\n      email: email,\n      password: pwd,\n      nickname: nickname,\n      name: uname\n    })\n\n    console.log(res);\n\n    if(res.data.status_code == 200){\n      alert('회원가입 성공! 환영합니다.')\n      navigate('/login');\n    } else {\n      console.log('sign up fail')\n    }\n  }\n\n  const onRegister = () => {\n    if(!email || !pwd || !nickname || !uname){\n      alert('모든 정보를 입력해주세요!')\n    } else {\n      sendRequest();\n    }\n  }\n\n  const navigate = useNavigate();\n  const goLogin = () => {\n    navigate('/login');\n  }\n\n  return (\n    <>\n      <RegisterDiv>\n\n        <RegTextComp />\n\n        <RegisterUl>\n\n          <RegisterLi>\n            <RegisterInputText>이름</RegisterInputText>\n            <RegisterInput \n              type=\"text\"\n              name=\"uname\"\n              value={uname}\n              ref={unameRef}\n              onChange={onChange}\n              onKeyUp={onKeyUp} />\n          </RegisterLi>\n\n          <RegisterLi>\n            <RegisterInputText>유저 닉네임</RegisterInputText>\n            <RegisterInput \n              type=\"text\"\n              name=\"nickname\"\n              value={nickname}\n              ref={nickRef}\n              onChange={onChange}\n              onKeyUp={onKeyUp} />\n          </RegisterLi>\n\n          <RegisterLi>\n            <RegisterInputText>이메일 주소</RegisterInputText>\n            { !isRightEmail && (\n              <WrongInputText>\n                올바른 이메일 표현식이 아닙니다.\n              </WrongInputText>\n            )}\n            <RegisterInput \n              type=\"text\"\n              name=\"email\"\n              value={email}\n              ref={emailRef}\n              onChange={onChange}\n              onKeyUp={onKeyUp} />\n          </RegisterLi>\n\n          <RegisterLi>\n            <RegisterInputText>비밀번호</RegisterInputText>\n            {!isPwdRight && (\n              <WrongInputText>\n                영문, 숫자 조합 8~16자를 입력해주세요.\n              </WrongInputText>\n            )}\n            {isPwdRight && isPwdDiff && (\n              <WrongInputText>\n                비밀번호가 일치하지 않습니다.\n              </WrongInputText>\n            )}\n            <RegisterInput \n              type=\"password\"\n              name=\"pwd\"\n              placeholder='영문, 숫자 조합 8~16자'\n              value={pwd}\n              ref={pwdRef}\n              onChange={onChange}\n              onKeyUp={onKeyUp} />\n\n            <RegisterInput \n              type=\"password\"\n              name=\"pwd2\"\n              placeholder='비밀번호 확인'\n              value={pwd2}\n              ref={pwd2Ref}\n              disabled={isPwdRight && pwd ? false : true}\n              onChange={onChange}\n              onKeyUp={onKeyUp} />\n          </RegisterLi>\n\n          {/* <RegisterLi>\n            <RegisterInputText>비밀번호 확인</RegisterInputText>\n            <RegisterInput \n              type=\"password\"\n              name=\"pwd2\"\n              placeholder='비밀번호 확인'\n              value={pwd2}\n              ref={pwd2Ref}\n              disabled={isPwdRight ? false : true}\n              onChange={onChange}\n              onKeyUp={onKeyUp} />\n          </RegisterLi> */}\n\n        </RegisterUl>\n\n        <RegisterBtn onClick={onRegister}>\n          가입하기\n        </RegisterBtn>\n\n        <AlreadyRegister onClick={goLogin}>\n          이미 회원이신가요?\n        </AlreadyRegister>\n\n      </RegisterDiv>\n    </>\n  );\n};\n\nexport default React.memo(Register);","/Users/changho/Desktop/LikeLion_Hackathon/client/devs/src/components/search/AdCard.jsx",["179","180","181","182","183","184","185"],[],"import React from 'react';\nimport 'bootstrap/dist/css/bootstrap.min.css';\nimport { Button, Form, Container, Nav, Navbar, NavDropdown } from 'react-bootstrap';\n\nimport './search.css';\n\nconst AdCard = () => {\n  return (\n    <section style={{display:'flex', flexDirection:'column', justifyContent:'space-evenly'}}className='ad-card'>   \n\n        <div className='float-container'>\n            <img className=\"profile-pic\" src={require('./profile-img.png')}></img>\n            <div>\n   